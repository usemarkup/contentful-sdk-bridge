{
    "name": "markup/contentful-sdk-bridge",
    "description": "A small bridge library to convert resources from the official Contentful SDK into resources compatible with the markup/contentful library.",
    "type": "library",
    "require": {
        "php": "^7.1",
        "markup/contentful": "dev-master",
        "contentful/contentful": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "symfony/phpunit-bridge": "^4.2",
        "mockery/mockery": "^1.2",
        "phpstan/phpstan-shim": "0.11.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Douglas Greenshields",
            "email": "douglas@usemarkup.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Markup\\ContentfulSdkBridge\\Tests\\": "tests/",
            "Markup\\ContentfulSdkBridge\\": "src/"
        }
    }
}
